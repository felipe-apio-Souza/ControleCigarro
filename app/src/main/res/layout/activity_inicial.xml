        android:layout_marginEnd="100dp"
        android:backgroundTint="#CA3E47"
        android:onClick="cigarro"
        android:text="Cigarro"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
